{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn"
  },
  "globals": {
    "g": true,
    "Bangle": true,
    "require": true,
    "setWatch": true,
    "BTN1": true
  }
}
